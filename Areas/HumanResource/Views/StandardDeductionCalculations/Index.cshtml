@model IEnumerable<dynamic>

<table class="table table-bordered table-responsive-sm">
    
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @item.Allowance
            </td>
            <td>
                <input type="checkbox" value="@item.Deductionid > 0 ? 1 : 0">
            </td>
        </tr>
}
    </tbody>
</table>
